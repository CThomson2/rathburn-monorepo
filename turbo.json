{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "@rathburn/ui#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@rathburn/mobile#build": {
      "dependsOn": ["@rathburn/ui#build", "^build"],
      "outputs": ["dist/**"]
    },
    "@rathburn/web#build": {
      "dependsOn": ["@rathburn/ui#build", "^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "!**/scripts/config-standalone.js"
      ],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false,
      "persistent": true
    }
  }
}
